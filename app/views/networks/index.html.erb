<div class="hero-unit">
  <h1>Discover local events near<br />your favourite places</h1>
  <p>
    <% @networks.each do |network| %>
      <%= link_to "Check out the preview", network, :class => "btn btn-primary btn-large" %>
    <% end %>
  </p>
</div>
<svg id="svg-image-blur">
    <filter id="blur-effect-1">
        <feGaussianBlur stdDeviation="2" />
    </filter>
</svg>